    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.4",
    "@scalar/nuxt": "^0.2.85",
    "@tanstack/vue-query": "^5.53.2",
    "@vueuse/core": "^11.0.3",
    "@vueuse/nuxt": "^11.0.3",
    "dayjs": "^1.11.13",
    "dayjs-nuxt": "^2.1.11",
    "globals": "^15.9.0",
    "nuxt": "^3.13.0",
    "nuxt-typed-router": "^3.6.5",
    "pinia": "^2.2.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@nuxtjs/color-mode": "^3.4.4",
