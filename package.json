{
  "name": "freddie-codeee",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "prettier --write \"./src/**/*.json\" \"./**/*.{html,js,ts,vue,css,md}\" \"./*.json\"",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --fix",
    "prepare": "husky && pnpm prepare:permissions",
    "prepare:permissions": "chmod ug+x .husky/*",
    "preview": "nuxt preview"
  },
  "lint-staged": {
    "*.{html,js,ts,vue,css,md}": "prettier --write",
    "*.{js,ts,vue}": "eslint --fix",
    "src/**/*.json": "prettier --write"
  },
  "dependencies": {
